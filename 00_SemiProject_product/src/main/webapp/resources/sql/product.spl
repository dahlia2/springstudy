DROP SEQUENCE PRODUCT_SEQ;
CREATE SEQUENCE PRODUCT_SEQ NOCACHE;

DROP TABLE PRODUCT;
CREATE TABLE PRODUCT (
    PROD_NO             NUMBER(6)             NOT NULL,        -- 상품 아이디
    PROD_NAME           VARCHAR2(60 BYTE)     NOT NULL,        -- 상품명(KR)
    PROD_NAME_ENG       VARCHAR2(60 BYTE)     NOT NULL,        -- 상품명(ENG)
    PROD_PRICE          NUMBER(10)            NOT NULL,        -- 가격
    PROD_CONTENT        CLOB                  NOT NULL,        -- 상품 설명
    PROD_IMG_WINE       VARCHAR2(100 BYTE)    NOT NULL,        -- 와인 이미지
    PROD_IMG_CONTENT    VARCHAR2(100 BYTE)    NOT NULL,        -- 내용 이미지
    PROD_TYPE           VARCHAR2(20 BYTE)     NOT NULL,        -- 종류 (RED, WHITE, SPARKLING, ROZE)
    PROD_NATION         VARCHAR2(100 BYTE)    NOT NULL,        -- 제조 국가
    PROD_BODY           VARCHAR2(30 BYTE)     NOT NULL,        -- 바디
    PROD_ALCOL          VARCHAR2(30 BYTE)     NOT NULL,        -- 알코올
    PROD_DATE           VARCHAR2(30 BYTE)     NOT NULL,        -- 등록일
    PROD_STOCK          NUMBER(10)            NOT NULL,        -- 재고
    
    CONSTRAINT PK_PRODUCT PRIMARY KEY(PROD_NO)
);

